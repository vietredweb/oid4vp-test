{
  "response_type": "vp_token",
  "client_id": "https://oid4vp-test.vercel.app",
  "redirect_uri": "https://oid4vp-test.vercel.app/api/callback",
  "scope": "openid",
  "state": "random123",
  "nonce": "nonce123",
  "presentation_definition": {
    "id": "age-country-check",
    "input_descriptors": [
      {
        "id": "age_check",
        "schema": {
          "uri": ["https://schema.org/Person"]
        },
        "constraints": {
          "fields": [
            {
              "path": ["$.birthDate"],
              "filter": {
                "type": "string",
                "format": "date",
                "maximum": "2007-08-01"
              }
            }
          ]
        }
      },
      {
        "id": "country_check",
        "schema": {
          "uri": ["https://schema.org/Person"]
        },
        "constraints": {
          "fields": [
            {
              "path": ["$.address.addressCountry"],
              "filter": {
                "type": "string",
                "const": "DE"
              }
            }
          ]
        }
      }
    ]
  }
}
